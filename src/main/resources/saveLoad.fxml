<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane fx:id="brothelAnchor" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.Sim.controllers.SaveLoadController">
    <children>
        <TableView fx:id="loadGameTable" layoutX="14.0" layoutY="14.0" prefHeight="639.0" prefWidth="1260.0">
            <columns>
                <TableColumn fx:id="saveNameColumn" editable="false" prefWidth="75.0" text="Name" />
                <TableColumn fx:id="saveDateColumn" editable="false" prefWidth="75.0" text="Date" />
                <TableColumn fx:id="saveTurnColumn" editable="false" prefWidth="75.0" text="Turn" />
                <TableColumn editable="false" prefWidth="75.0" />
                <TableColumn editable="false" prefWidth="75.0" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
        <Button layoutX="25.0" layoutY="674.0" mnemonicParsing="false" onAction="#goToMain" text="Main menu" />
        <Button layoutX="160.0" layoutY="674.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <Button layoutX="1038.0" layoutY="674.0" mnemonicParsing="false" onAction="#save" prefHeight="46.0" prefWidth="182.0" text="New Save" />
    </children>
</AnchorPane>
